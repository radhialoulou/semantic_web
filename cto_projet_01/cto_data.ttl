# tour packages

cto:GrandEuropeanTour a ctosch:TourPackage ;
    rdfs:label "Grand European Tour"@en ;
    ctosch:includesPath cto:SeineRiverTrail , cto:GalibierClimb ;
    ctosch:duration "10"^^xsd:integer ;
    ctosch:assignedGuide cto:Guide_JulieMartin ;
    ctosch:includesBikeType cto:CityBike , cto:RoadBike ;
    ctosch:price "1800"^^xsd:float ;
    ctosch:maxParticipants "12"^^xsd:integer ;
    ctosch:startDate "2025-05-01"^^xsd:date ;
    ctosch:endDate "2025-05-10"^^xsd:date ;
    ctosch:availableOptionalActivity cto:SeineBoatTour .

cto:UltimateCyclingChallenge a ctosch:TourPackage ;
    rdfs:label "Ultimate Cycling Challenge"@en ;
    ctosch:includesPath cto:SeineRiverTrail , cto:GalibierClimb , cto:SaharaAdventureRoute ;
    ctosch:duration "15"^^xsd:integer ;
    ctosch:assignedGuide cto:Guide_JulieMartin ;
    ctosch:includesBikeType cto:CityBike , cto:RoadBike , cto:MountainBike ;
    ctosch:price "3200"^^xsd:float ;
    ctosch:maxParticipants "8"^^xsd:integer ;
    ctosch:startDate "2025-06-01"^^xsd:date ;
    ctosch:endDate "2025-06-15"^^xsd:date ;
    ctosch:availableOptionalActivity cto:SeineBoatTour , cto:AlpsPhotographyWorkshop , cto:TunisiaDesertSafari .

cto:DesertAdventureTour a ctosch:TourPackage ;
    rdfs:label "Desert Adventure Tour"@en ;
    ctosch:includesPath cto:SaharaAdventureRoute ;
    ctosch:duration "7"^^xsd:integer ;
    ctosch:assignedGuide cto:Guide_AhmedBenAli ;
    ctosch:includesBikeType cto:MountainBike , cto:TrekkingBike ;
    ctosch:price "1500"^^xsd:float ;
    ctosch:maxParticipants "10"^^xsd:integer ;
    ctosch:startDate "2025-03-15"^^xsd:date ;
    ctosch:endDate "2025-03-22"^^xsd:date .


# optional activities

cto:SeineBoatTour a ctosch:TourOption ;
    rdfs:label "Seine River Boat Tour"@en ;
    rdfs:comment "Optional boat ride along the Seine in Paris."@en ;
    ctosch:optionPrice "50"^^xsd:float ;
    ctosch:relatedDestination cto:Paris .

cto:AlpsPhotographyWorkshop a ctosch:TourOption ;
    rdfs:label "Alps Photography Workshop"@en ;
    rdfs:comment "Optional guided photography workshop in the French Alps."@en ;
    ctosch:optionPrice "120"^^xsd:float ;
    ctosch:relatedDestination cto:FrenchAlps .

cto:TunisiaDesertSafari a ctosch:TourOption ;
    rdfs:label "Tunisia Desert Safari"@en ;
    rdfs:comment "Optional 4x4 desert safari adventure in Tunisia."@en ;
    ctosch:optionPrice "200"^^xsd:float ;
    ctosch:relatedDestination cto:Tunisia .

