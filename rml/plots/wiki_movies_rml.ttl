@base <http://saraaymericradhi.org/rml/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix movie: <http://saraaymericradhi.org/movie-ontology#> .

# =============================================================================
# SOURCE: Wiki Movie Plots Top 200 (CSV)
# =============================================================================

<#WikiMoviePlotMap>
    rml:logicalSource [
        rml:source "../../movies/wiki_movie_plots_top200.csv" ;
        rml:referenceFormulation ql:CSV
    ] ;
    
    rr:subjectMap [
        rr:template "http://saraaymericradhi.org/movie/{id}" ;
        rr:class movie:Movie
    ] ;
    
    # Wikipedia Page
    rr:predicateObjectMap [
        rr:predicate movie:hasWikipediaPage ;
        rr:objectMap [ rml:reference "Wiki Page" ; rr:datatype xsd:anyURI ]
    ] .